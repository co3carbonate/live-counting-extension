{
	"compilerOptions": {
		"allowJs": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"lib": [
			"es2020",
			"dom"
		],
		"module": "es2020",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "./dist",
		"resolveJsonModule": true,
		"target": "es5",
		"types": [
			"@types/events",
			"@types/jquery",
			"@types/webpack-env"
		]
	}
}